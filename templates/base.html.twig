<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('styles/app.css') }}" rel="stylesheet">
        <link href="{{ asset('styles/components/buttons.css') }}" rel="stylesheet">
        <link href="{{ asset('styles/sections/detail.css') }}" rel="stylesheet">
        <link href="{{ asset('styles/sections/a-venir.css') }}" rel="stylesheet">
        <link href="{{ asset('styles/sections/calendar.css') }}" rel="stylesheet">
        <link href="{{ asset('styles/sections/add-modif.css') }}" rel="stylesheet">
        <link href="{{ asset('styles/pages/login.css') }}" rel="stylesheet">
        <link href="{{ asset('styles/pages/liste.css') }}" rel="stylesheet">
    {% endblock %}

    {% block javascripts %}
        <script src='{{ asset('fullcalendar-6.1.10/dist/index.global.min.js') }}'></script>
        <script src='{{ asset('fullcalendar-6.1.10/packages/core/locales/fr.global.min.js') }}'></script>
        <script src="https://kit.fontawesome.com/9d34ec166c.js" crossorigin="anonymous"></script>
        <script src="{{ asset('js/details--open.js') }}"></script>
        {% block importmap %}{{ importmap('app') }}{% endblock %}
    {% endblock %}
</head>
<body class="{% block body_class %}sub_page{% endblock %}">
{% block body %}{% endblock %}
</body>
</html>
